<script type="text/javascript">
var sskey = '1oZ2NEmoa0XHSGTWKaBLt0DJK1kIbpWO6iSZ0I2OE2gA';

function setSite(site){
  document.getElementById("network").value = site.substring(0,2) + "CLIMATE";

} /* End of setSite() */

function showOneSection(toshow) {
    var sections = [ 'loading', 'main', 'approval', 'waiting' ];
    for (var i=0; i < sections.length; ++i) {
        var s = sections[i];
        var el = document.getElementById(s);
        if (s === toshow) {
            el.style.display = "block";
        } else {
            el.style.display = "none";
        }
    }
}

function loadSites(feed){
    /* Load up the sites and the associated weather station */
    var sel = document.getElementById('station');
    var list = feed.entry;
    for(var i=0, n=list.length; i < n; i++) {
        var entry = list[i];
        var iem = entry.gsx$iemclimatesite.$t;
        var tokens = iem.split(/ /);
        if (tokens.length < 2 || tokens[0].length != 6){ continue; }
        var opt = document.createElement("option");
        opt.text = entry.gsx$uniqueid.$t +" ["+ entry.gsx$citynearest.$t +", "+ entry.gsx$state.$t +"] -- "+ entry.gsx$iemclimatesite.$t;
        opt.value = tokens[0];
        sel.options.add( opt ); 
    } /* End of for loop */
} /* End of loadSites() */

/* Called once google has all our apis loaded! */
function bootstrap(){
   url = 'https://spreadsheets.google.com/feeds/list/'+ sskey +'/1/private/full';
   var callback = function(response) {
         if (response.oauthApprovalUrl) {
             var personalize = document.getElementById('personalize');
             personalize.href = response.oauthApprovalUrl;
             showOneSection('approval');
         } else if (response.feed) {
             showOneSection('main');
             loadSites(response.feed);
         } else {
             var main = document.getElementById('main');
             var err = document.createTextNode('Something went wrong');
             main.appendChild(err);
             showOneSection('main');
         }
    };
    service = new google.gdata.client.GoogleService('testapp');
    service.useOAuth('google');
    service.getFeed(url, callback, callback);
} /* End of bootstrap() */

/* called after gadgets are ready, now we need more apis */
function myinit(){
  google.load("gdata", "2.x");
  google.load("jquery", "1.7.1");
  google.load("jqueryui", "1.8.16");
  google.setOnLoadCallback(bootstrap);
}

/* Wait until google gadgets is fired up */
gadgets.util.registerOnLoadHandler(myinit);

</script>
